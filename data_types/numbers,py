print(type("char")) #type of command
print('int')
print(9.9+1.1) # FLoat values
print(5//4) # round of to integer
print(20%6)




#-- MATH functions
print (round(3.23674,2)) # round off to 2 decimal places
print (abs(-20)) #non -ve



print (bin(8))
print (hex(445))
print (int('0x1bd',16))
print (int('0b1111',2))




